                    },
                    {
                      "text": "{{judgeName}}",
                      "style": "signature"
                    },
                    {
                      "text": "Judge, {{courtName}}",
                      "style": "signature"
                    },
                    {
                      "text": "{{courtSeal}}",
                      "style": "signature"
                    }
                  ],
                  "alignment": "left",
                  "margin": [0, 20, 0, 20]
                }
              ]
            ]
          },
          "layout": "noBorders"
